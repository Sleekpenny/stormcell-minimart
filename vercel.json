    "buildCommand": "ng build --configuration production",
    "installCommand": "npm install --legacy-peer-deps",
    "outputDirectory": "dist/stormcell-minicart/browser",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }