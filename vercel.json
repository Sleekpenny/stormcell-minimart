{
    "version": 2,
    "buildCommand": "ng build --configuration production",
    "installCommand": "npm install --legacy-peer-deps",
    "outputDirectory": "dist/stormcell-minicart/browser",
    "framework": null,
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|webp|woff2?))",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }